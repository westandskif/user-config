# cat org_gnome_settings_daemon_plugins_media_keys_custom_keybindings_dir.backup | dconf load /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/
# dconf write /org/gnome/settings-daemon/plugins/media-keys/custom-keybindings "$(cat org_gnome_settings_daemon_plugins_media_keys_custom_keybindings.backup)"
