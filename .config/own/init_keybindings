#!/bin/bash

dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-1 "['<Shift><Super>F1']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-2 "['<Shift><Super>F2']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-3 "['<Shift><Super>F3']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-4 "['<Shift><Super>F4']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-5 "['<Shift><Super>F5']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-6 "['<Shift><Super>F6']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-7 "['<Shift><Super>F7']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-8 "['<Shift><Super>F8']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-9 "['<Shift><Super>F9']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-10 "['<Shift><Super>F10']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-11 "['<Shift><Super>F11']"
dconf write /org/gnome/desktop/wm/keybindings/move-to-workspace-12 "['<Shift><Super>F12']"

dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-1 "['<Super>F1']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-2 "['<Super>F2']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-3 "['<Super>F3']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-4 "['<Super>F4']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-5 "['<Super>F5']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-6 "['<Super>F6']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-7 "['<Super>F7']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-8 "['<Super>F8']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-9 "['<Super>F9']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-10 "['<Super>F10']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-11 "['<Super>F11']"
dconf write /org/gnome/desktop/wm/keybindings/switch-to-workspace-12 "['<Super>F12']"

